<panel navigation_container="true">
	<container id="SUBP">
		<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="8" />
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" />
	</container>

	<widget class="popup_button" id="popv" auto_size="horizontal" height="20" click_event="msis" appearance="buttons/custom/customise panel/button" fixed="true">
		<layout class="stick_to_sides_attachment" alignment="right,top" inset="12,8" />
	</widget>

	<widget class="club_overview_kits_panel" file="none" id="it2k" save_session_state="true">
		<record id="object_property" get_property="Pclb" set_property="objt"/>
		<layout class="arrange_vertical_attachment" alignment="middle,extend" offset="0"/>
		<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" />
        <translation id="title" type="use" value=" Form (Last 10 Matches)" />

		<!-- We are using a spacer here so we can limit our images size, but still remain centered in our parent -->
		<widget class="spacer" height="1" />

		<container>
			<attachment_group class="horizontal_arrange" horizontal_alignment="fill" horizontal_offset="0" horizontal_gap="0" vertical_inset="2" />

			<record id="default_properties" scale_picture="true" keep_aspect_ratio="true" image_alignment="right" always_use_latest_object_list="true">
				<record id="object_property">
					<integer id="get_property" value="Tidx" />
					<integer id="set_property" value="indx" />
				</record>
			</record>

			<widget class="kit_picture" id="T_hk" kit_id="0" />
		</container>
	</widget>

</panel>